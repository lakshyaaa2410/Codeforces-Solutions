    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        ll n;
        cin >> n;
     
        if (n % 2 != 0)
            cout << - 1 << nline;
        else
            cout << 0 << " " << 0 << " " << n / 2 << nline;
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        cin >> T;
        while (T--) 
        {
            solve();
        }
    }
