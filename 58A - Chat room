    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        string s, patt = "hello";
        cin >> s;
     
        bool flag = false;
        int j = 0;
        for (int i = 0; i < s.size(); i++)
        {
            if (s[i] == patt[j])
            {
                j++;
                if (j == patt.size())
                {
                    flag = true;
                    break;
                }
            }
        }
     
        if (flag)
            cout << "YES" << nline;
        else
            cout << "NO" << nline;
     
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        // cin >> T;
        while (T--) 
        {
            solve();
        }
     
    }
