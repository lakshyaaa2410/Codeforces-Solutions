#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    ll a, b;
    cin >> a >> b;

    if (b == 1)
        cout << "No\n";
    else
        cout << "Yes\n" << a << " " << (a * b) << " " << a * (b + 1) << nline;
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}
