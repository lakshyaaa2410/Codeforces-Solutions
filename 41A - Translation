    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        string s, t;
        cin >> s >> t;
        reverse(s.begin(),  s.end());
        
        if (s == t)
            cout << "YES\n";
        else
            cout << "NO\n";
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        while (T--) 
        {
            solve();
        }
     
    }
