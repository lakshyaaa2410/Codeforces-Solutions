    #include <bits/stdc++.h>
    #define ll long long int
    #define nline '\n'
    using namespace std;
     
    void solve()
    {
        ll n;
        cin >> n;
     
        for (int i = 2; i < 30; i++)
        {
            int pow = (1 << i) - 1;
            if (n % pow == 0)
            {
                cout << n / pow << nline;
                break;
            }
        }
    }
     
    int main()
    {
        ios_base::sync_with_stdio(false);
        cin.tie(NULL);
        ll T = 1;
        cin >> T;
        while (T--) 
        {
            solve();
        }
     
    }
