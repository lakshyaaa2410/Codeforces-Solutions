#include <bits/stdc++.h>
#define ll long long int
#define nline '\n'
using namespace std;

void solve()
{
    string s, t;
    cin >> s >> t;

    ll n = s.size(), m = t.size();

    if (s[0] == t[0]) {
        cout << "YES\n";
        cout << s[0] << "*\n";
    }
    else if (s[n - 1] == t[m - 1]) {
        cout << "YES\n";
        cout << "*" << s[n - 1] << nline;
    }
    else
    {
        for (int i = 0; i < n - 1; i++)
        {
            for (int j = 0; j < m - 1; j++)
            {
                if (s[i] == t[j] and s[i + 1] == t[j + 1])
                {
                    cout << "YES\n";
                    cout << "*" << s[i] << s[i + 1] << "*\n";
                    return;
                }
            }
        }
        cout << "NO\n";
    }
}

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    ll T = 1;
    cin >> T;
    while (T--) 
    {
        solve();
    }

}
